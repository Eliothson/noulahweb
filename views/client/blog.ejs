<%- include('header'); -%>

<section class="services2 cid-rZ5Gu7N4i2" id="services2-f">
    
    <!--Container-->
    <% if (locals.firstelement) {%>
    <div class="container">
        <div class="col-md-12">
            <div class="media-container-row">
                <% if (firstelement.lienImage !=="false") {%>
                    <% if (firstelement.mediatype =="image/jpeg" || firstelement.mediatype =="image/png" ){%>
                    <div class="mbr-figure" style="width: 50%;">
                        <img src="<%= firstelement.lienImage %>" alt="noulha">
                    </div>
                    <% } else { %>
                        <div class="mbr-figure" style="width: 50%;">
                            <video class="video-block" style="width: 80%" src="<%= firstelement.lienImage %>" controls  alt="image Matitech">
                        </div>
                    <% }
                    } %>
                <div class="align-left aside-content">
                    <h2 class="mbr-title pt-2 mbr-fonts-style display-2">
                        <%= firstelement.titre %>
                    </h2>
                    <div class="mbr-section-text">
                        <a class="mbr-text text1 pt-2 mbr-light mbr-fonts-style display-7">
                            <%= firstelement.link%>
                        </a>
                        <p class="mbr-text text1 pt-2  mbr-fonts-style display-7">
                            <%= firstelement.message%>
                        </p>
                    </div>
                    <!--Btn-->
                    <div class="mbr-section-btn pt-3 align-left"><a href="/" class="btn btn-warning-outline display-4">
                            Aimer</a></div>
                </div>
            </div>
        </div>
        <% } %>
    </div>
</section>

